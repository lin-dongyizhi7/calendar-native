{
  "compilerOptions": {
    "target": "es2019",
    "module": "esnext",
    "lib": ["es2019"],
    "jsx": "react-jsx",
    "allowJs": false,
    "checkJs": false,
    "noEmit": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "."
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}


